{
  "$schema": "https://qooxdoo.org/schema/Manifest-2-0-0.json",
  "info" :
  {
    "name" : "<%= ${class} %>",
    "summary" : "<%= ${class} %> web app",
    "description" : "A generic Callbackery app.",
    "homepage" : "https://your-homepage",
    "license" : "???",
    "authors" : [
        {
            "name" : "<%= "${fullName}" %>",
            "email" : "<%= ${email} %>"
          }
    ],
    "version" : "0.0.1"
  },
  "provides" :
  {
    "namespace"   : "<%= ${class_file} %>",
    "encoding"    : "utf-8",
    "class"       : "source/class",
    "resource"    : "source/resource",
    "translation" : "source/translation"
  },
  "requires": {
    "@qooxdoo/framework": "^7.0.0",
    "qooxdoo/deprecated.qx.io.remote": "^1.0.0"
  }
}
